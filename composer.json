{
  "name": "chamber-orchestra/doctrine-extensions-bundle",
  "type": "library",
  "description": "Symfony Doctrine extensions bundle for PostgreSQL",
  "keywords": [
    "symfony",
    "doctrine-extensions-bundle",
    "doctrine-extensions",
    "doctrine",
    "postgresql"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Lukin",
      "email": "lukin.andrej@gmail.com",
      "homepage": "https://github.com/wtorsi",
      "role": "Developer"
    }
  ],
  "require": {
    "ext-intl": "*",
    "php": "^8.5",
    "symfony/dependency-injection": "8.0.*",
    "symfony/config": "8.0.*",
    "symfony/framework-bundle": "8.0.*",
    "symfony/runtime": "8.0.*",
    "symfony/options-resolver": "8.0.*",
    "symfony/clock": "8.0.*",
    "symfony/uid": "8.0.*",
    "doctrine/orm": "^3.0",
    "doctrine/common": "^3.0",
    "doctrine/doctrine-bundle": "3.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^13.0",
    "symfony/test-pack": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "ChamberOrchestra\\DoctrineExtensionsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "8.0.*"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
